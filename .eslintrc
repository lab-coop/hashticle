{
  "extends": [
    "eslint:recommended", "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "react/jsx-indent-props": 1,
    "no-console": 0,
    "react/jsx-indent": [2,2]
  }
}
